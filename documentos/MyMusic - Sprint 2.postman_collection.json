					"script": {
						"type": "text/javascript",
						"exec": [
							"pm.sendRequest({",
							"    url:  \"http://localhost:8080/api/v1/token\",",
							"    method: \"POST\",",
							"    header: {",
							"        \"Content-Type\": \"application/json\"      ",
							"    },",
							"    body: JSON.stringify({",
							"        data: {",
							"            name: \"Gustavo\"",
							"        }",
							"    }) ",
							"}, function (err, res) {",
							"    var arr = new Uint8Array(res.stream);",
							"    var token = String.fromCharCode.apply(String, arr);   ",
							"    pm.environment.set(\"token\", token);",
							"});"
						]
					}
